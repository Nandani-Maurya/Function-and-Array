import java.io.*;
import java.util.*;
public class Main
{
    public static void main (String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr[] = new int[n];
        for(int i = 0; i<arr.length; i++)
        {
            arr[i] = sc.nextInt();
        }
        int a[] = new int[n];
        for(int i = 0; i< arr.length; i++)
        {
            int j =arr[i];
            a[j]=i;
            
        }
        for(int i =0; i < arr.length; i++)
        {
            System.out.println(a[i]);
        }
    }
}
